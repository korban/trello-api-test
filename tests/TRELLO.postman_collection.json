		{
			"name": "Create a board",
			"event": [
				{
					"listen": "test",
					"script": {
						"id": "72b1c182-65f2-4fa4-ae11-0a7a2b723f32",
						"type": "text/javascript",
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"",
							"pm.test(\"Check board name\", function () {",
							"    var jsonData = pm.response.json();",
							"    pm.expect(jsonData.name).to.eql(pm.environment.get(\"boardName\"));",
							"});",
							"",
							"// intercept json response",
							"var response = pm.response.json();",
							"",
							"// create in variable with id board",
							"pm.environment.set(\"id_board\", response.id);",
							""
						]
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"id": "be4d1483-a0f4-44bb-9f5b-5c5a8a9e1cec",
						"type": "text/javascript",
						"exec": [
							"",
							"var counter = pm.environment.get(\"counter\");",
							"var boardNameTemplate = pm.environment.set(\"boardNameTemplate\", \"apiTest \");",
							"",
							"// if can't get a variable from env., set it = 0",
							"if(counter === undefined) {",
							"    counter = 0;",
							"}",
							"",
							"// increase the counter if counter has in env.",
							"counter += 1;",
							"// set it to env.",
							"pm.environment.set(\"counter\", counter);",
							"",
							"// create a board name with static template and counter",
							"var boardName = pm.environment.get(\"boardNameTemplate\") + pm.environment.get(\"counter\");",
							"",
							"pm.environment.set(\"boardName\", boardName);",
							"",
							"",
							"",
							"/*another variant",
							"",
							"",
							"// Use currentBoardNumber to store the board number",
							"var currentBoardNumber = postman.getEnvironmentVariable(\"currentBoardNumber\");",
							" ",
							"// Check if currentBoardNumber is already defined",
							"if (currentBoardNumber) {",
							"    currentBoardNumber++; // increment",
							"} else {",
							"    currentBoardNumber = 1; // start from 1",
							"}",
							" ",
							" ",
							"var boardName = \"My board \" + currentBoardNumber;",
							" ",
							"// Set the boardName for the request",
							"postman.setEnvironmentVariable(\"boardName\", boardName);",
							" ",
							"// Set the currentBoardNumber for future requests",
							"postman.setEnvironmentVariable(\"currentBoardNumber\", currentBoardNumber);",
							"*/"
						]
					}
				}
			],
